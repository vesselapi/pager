{
  "name": "@vessel/expo",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev:mobile": "pnpm with-env expo start --ios",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "with-env": "cp ../../.env .env && "
  },
  "dependencies": {
    "@clerk/clerk-expo": "^0.19.21",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^5.8.1",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "date-fns": "^2.30.0",
    "expo": "^49.0.16",
    "expo-auth-session": "^5.2.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.12",
    "expo-device": "~5.4.0",
    "expo-linking": "~5.0.2",
    "expo-notifications": "~0.20.1",
    "expo-router": "2.0.11",
    "expo-secure-store": "^12.5.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "^1.6.0",
    "expo-web-browser": "^12.5.0",
    "nativewind": "^4.0.13",
    "radash": "^11.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "superjson": "2.2.0",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@expo/config-plugins": "^7.2.5",
    "@types/babel__core": "^7.20.4",
    "@types/react": "^18.2.37",
    "@vessel/api": "workspace:^0.1.0",
    "@vessel/eslint-config": "workspace:^0.2.0",
    "@vessel/prettier-config": "workspace:^0.1.0",
    "@vessel/tailwind-config": "workspace:^0.1.0",
    "@vessel/tsconfig": "workspace:^0.1.0",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.5",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@vessel/eslint-config/base",
      "@vessel/eslint-config/react"
    ],
    "ignorePatterns": [
      "expo-plugins/**"
    ]
  },
  "prettier": "@vessel/prettier-config"
}
